#include<stdio.h>
int main()
{	//输入
	int arr[10] = { 0 };
	int i = 0;

	for (i = 0; i < 10; i++)
	{
		scanf_s("%d", &arr[i]);
	}
	//检查 对比 

	int max = arr[0];
	for(i=1;i<10;i++)
	{
		if (max < arr[i])
		{
			max = arr[i];

		}
	}
	
	//输出
	for (i = 1; i < 10; i++)
	{
		

	}
	printf("max=%d", max);


return 0;
}
